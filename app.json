{
  "expo": {
    "name": "Noisera",
    "slug": "noisera",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Noisera is a React Native application to organize musical gigs. You can store songs letters, tabs, sheets, and set its characteristics, like tone, anormal interruptions, and much another things.",
    "githubUrl": "https://github.com/vitorlofonseca/NoiseraApp"
  }
}